// codegen


fn range(a: int, z: int, body: fn(int)->()) -> () {
    if a < z {
        body(a);
        range(a+1, z, body)
    }
}

fn fib(n: int, body: fn(int)->()) -> () {
    let ret = return;
    fn loop(n: int, cur: int, prev: int) -> ! {
        if (n != 0) {
            body(cur, || -> ! loop(n-1, cur + prev, cur) )
        } else {
            ret()
        }
    }
    loop(n, 1, 0)
}

fn main() -> int {
    let mut result: int = 0;
    // Stack overflows otherwise; this is not *really* an error without PE
    for i in range(0, /* 15000000 */ 150000) {
        fib(13,
            |f: int| result = result % 4 + f,
            continue
        )
    }
    if result == 236 { 0 } else { 1 }
}
