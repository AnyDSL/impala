fn main() -> int {
    let mut a = 1;
    let a_dec_zero = atomic_dec(&a);

    let mut b = 9;
    let b_dec_zero = atomic_dec(&b);

    if a_dec_zero && !b_dec_zero && a == 0 && b == 8 { 0 } else { 1 }
}

fn atomic_dec(p: &i32) -> bool {
    let mut res: bool = false;
    asm ("lock; decl $1; sete $0;"
            : /*"=r" (p),*/ "=q" (res)
            : "*m" (p) 
            : "memory"
            : "volatile"
        );
    res
}
