/*
foreach that adds all primes smaller than 40
expected result: 197
*/
fn primes(n: int, body: fn(int) -> ()) -> () {
    let ret = return;
    if n < 2 {
        ret()
    }

    body(2, || -> ! {
        fn loop(i: int) -> ! {
            fn inner_loop(j: int) -> ! {
                if j < i {
                    if i % j == 0 { 
                        loop(i + 1)
                    } else { 
                        inner_loop(j + 1)
                    }
                } else {
                    body(i, || -> ! loop(i+1))
                }
            }
            if i < n {
                inner_loop(2)
            } else {
                ret()
            }
        }
        loop(3)
    })
}

fn main() -> int {
    let mut result = 0;
    for p in primes(40) {
        result += p;
    }
    if result == 197 { 0 } else { 1 }
}
