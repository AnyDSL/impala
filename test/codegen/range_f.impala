fn range(a: int, b: int, body: fn(int)->()) -> () {
    if a < b {
        body(a);
        range(a+1, b, body, return)
    } else {
        return()
    }
}

fn main() -> int {
    let mut sum = 0;
    fn body(i: int) -> () {
        sum += i
    }
    range(0, 10, body);
    if sum == 45 { 0 } else { 1 }
}
