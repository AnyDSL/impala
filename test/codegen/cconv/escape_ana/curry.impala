
extern fn curry(f:fn(i32, i32)->i32) -> fn(i32) -> fn(i32) -> i32 {
    fn f1 (x:i32) -> fn(i32) -> i32 { 
        fn f2 (y: i32) { f(x, y) }
        f2
    }
    f1
}

extern fn use(f:fn(i32)->i32) -> i32 {
    let closure = |x:i32,y:i32|->i32 f(x+y);
    let curried = curry(closure);
    curried(0)(1)
}
