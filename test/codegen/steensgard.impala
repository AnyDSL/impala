fn myfun(i: int) -> int {
    let ret = return;
    fn branch(cond: bool, t: fn() -> !, f: fn() -> !) -> ! {
        if cond { t() } else { f() }
    }

    fn switch(i: int, a: fn() -> !, b: fn() -> !, c: fn() -> !) -> ! {
        if i == 0 {
            a()
        } else if i == 1 {
            b()
        } else {
            c()
        }
    }

    fn x() -> ! { z() }
    fn y() -> ! { z() }
    fn z() -> ! { e() }

    fn b() -> ! { d() }
    //fn c() { e() }
    fn c() -> ! { branch(i == 0, x, y) }
    fn d() -> ! { switch(i, b, e, f) }
    fn e() -> ! { switch(i, d, c, f) }
    fn f() -> ! { ret(23) }

    branch(i == 0, b, c)
}

fn main() -> int {
    if myfun(2) == 23 { 0 } else { 1 }
}
