fn @(?a & ?b) range(a: int, b: int, body: fn(int, fn() -> !) -> !) -> () {
    if a < b {
        body(a);
        range(a+1, b, body)
    }
}

fn @foo(n: int) -> int {
    let mut m = 0;
    let mut res = 0;
    for i in range(0, n) @{
        m++;
        for j in range(0, m) @{
            res++;
        }
    }
    res
}

fn main() -> int {
    foo(2)
}
