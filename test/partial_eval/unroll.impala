fn range(a: int, b: int, body: fn(int, fn() -> !) -> !) -> () {
    if a < b {
        body(a);
        range(a+1, b, body)
    }
}

fn foo(i: int) -> float {
    if i == 0 { 23.f } else { 42.f }
}

fn main(n: int) -> () {
    let mut arr = ~[n: float];

    let unroll = 100;
    for i in @range(0, unroll) {
        arr(0) = foo(i);
    }
}
