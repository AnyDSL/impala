fn range(a: int, b: int, body: fn(int, fn() -> !) -> !) -> () {
    if a < b {
        body(a);
        range(a+1, b, body)
    }
}

fn foo(a: int, b: int) -> int {
    let mut res1 = 0;
    for i in range(7, a) {
        res1 += 666;
    }

    let mut res2 = 0;
    for i in range(17, b) {
        res2 += 3333;
    }

    17 + res2
}

fn main(i: int) -> int {
    @foo(i, 19)
}
